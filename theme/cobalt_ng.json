{
  "$schema": "vscode://schemas/color-theme",
  "name": "Cobalt Theme NG",
  "type": "dark",

  "semanticHighlighting": true,
  "semanticTokenColors": {
    "function": {
      "foreground": "#FFC600"
    },
    "variable": "#9EFFFF",
    "enumMember": "#FF68B8",
    "macro": "#569CD6",
    "parameter": "#9CDCFE",
    "property": "#FB94FF",
    "selfParameter": {
      "foreground": "#822287",
      "fontStyle": "bold"
    },
    "magicFunction": {
      "foreground": "#822287",
      "fontStyle": "bold"
    },
    "interface": {
      "foreground": "#FF68B8",
    },
    "type": {
      "foreground": "#80FFBB",//"#FFC600",
    },
  },

