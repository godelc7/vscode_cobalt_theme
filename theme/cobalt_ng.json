    "panel.border": "#000000",//"#FFC600",
    "panelTitle.activeBorder": "#000000",//"#FFC600",
    "panelTitle.activeForeground": "#FFFFFF",//"#FFC600",
    "panelTitle.inactiveForeground": "#AAAAAA",

    // ---------------------------------- peekView -----------------------------
    "peekView.border": "#FFC600",
